{
    "manifest_version": 2,
    "name": "90sTube",
    "version": "1.0",
    "permissions": ["storage", "tabs"],
    "host_permissions": ["https://*.youtube.com/"],
    "description": "Simplified, retro-infused iTunes lay-out",
    "content_scripts": [
        {
            "matches": [
                "https://*.youtube.com/"
            ],
            "js": [
                "main.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}